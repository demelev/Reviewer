html
  head
    link(rel="stylesheet" href="http://yui.yahooapis.com/pure/0.6.0/pure-min.css")
    style
      include style/main.css

  body
    a(href='/members') Back to members list
    table(class="pure-table pure-table-bordered", style='width:100%; margin-top: 30px;')
      thead: tr
        td Reviewer
        td(style='width:70%;') Feedback
      tbody
        -each reviewer in Object.keys(reviewers.responses)
          -if(reviewers.responses[reviewer].feedback)
            tr
              td= reviewer
              td= reviewers.responses[reviewer].feedback
